				"textures": {
				  "Array.tame": [
						"Texture.shroom_wild_awake_variant_0",
						"Texture.shroom_wild_sleep_variant_1",
						"Texture.shroom_tamed_variant_2"
					],
					"Array.skins": [
						"Texture.shroom_wild_awake_variant_0",
						"Texture.shroom_wild_sleep_variant_1"
					]
				}
			}
		}
	}
}